load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "teo",
    srcs = ["teo.go"],
    importpath = "github.com/go-playground/locales/teo",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
