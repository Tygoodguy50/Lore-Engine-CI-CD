load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "xog",
    srcs = ["xog.go"],
    importpath = "github.com/go-playground/locales/xog",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
