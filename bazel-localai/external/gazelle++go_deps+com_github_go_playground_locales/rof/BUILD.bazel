load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "rof",
    srcs = ["rof.go"],
    importpath = "github.com/go-playground/locales/rof",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":rof",
    visibility = ["//visibility:public"],
)

go_test(
    name = "rof_test",
    srcs = ["rof_test.go"],
    embed = [":rof"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
