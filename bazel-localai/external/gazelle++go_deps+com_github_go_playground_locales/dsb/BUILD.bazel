load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "dsb",
    srcs = ["dsb.go"],
    importpath = "github.com/go-playground/locales/dsb",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
