load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "eo",
    srcs = ["eo.go"],
    importpath = "github.com/go-playground/locales/eo",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
