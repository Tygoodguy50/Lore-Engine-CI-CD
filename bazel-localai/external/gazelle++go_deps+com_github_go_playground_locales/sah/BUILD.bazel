load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "sah",
    srcs = ["sah.go"],
    importpath = "github.com/go-playground/locales/sah",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":sah",
    visibility = ["//visibility:public"],
)

go_test(
    name = "sah_test",
    srcs = ["sah_test.go"],
    embed = [":sah"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
