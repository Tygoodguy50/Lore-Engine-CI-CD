load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "fa",
    srcs = ["fa.go"],
    importpath = "github.com/go-playground/locales/fa",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":fa",
    visibility = ["//visibility:public"],
)

go_test(
    name = "fa_test",
    srcs = ["fa_test.go"],
    embed = [":fa"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
