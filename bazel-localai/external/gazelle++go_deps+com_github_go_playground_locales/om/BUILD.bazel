load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "om",
    srcs = ["om.go"],
    importpath = "github.com/go-playground/locales/om",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":om",
    visibility = ["//visibility:public"],
)

go_test(
    name = "om_test",
    srcs = ["om_test.go"],
    embed = [":om"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
