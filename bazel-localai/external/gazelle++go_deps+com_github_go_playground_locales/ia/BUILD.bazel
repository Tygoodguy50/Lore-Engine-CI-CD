load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "ia",
    srcs = ["ia.go"],
    importpath = "github.com/go-playground/locales/ia",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
