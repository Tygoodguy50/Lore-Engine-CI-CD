load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "mi",
    srcs = ["mi.go"],
    importpath = "github.com/go-playground/locales/mi",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":mi",
    visibility = ["//visibility:public"],
)

go_test(
    name = "mi_test",
    srcs = ["mi_test.go"],
    embed = [":mi"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
