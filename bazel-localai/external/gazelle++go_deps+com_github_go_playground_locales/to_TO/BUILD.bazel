load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "to_TO",
    srcs = ["to_TO.go"],
    importpath = "github.com/go-playground/locales/to_TO",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":to_TO",
    visibility = ["//visibility:public"],
)

go_test(
    name = "to_TO_test",
    srcs = ["to_TO_test.go"],
    embed = [":to_TO"],
    deps = [
        "//:locales",
        "//currency",
    ],
