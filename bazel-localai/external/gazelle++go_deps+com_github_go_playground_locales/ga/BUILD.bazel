load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "ga",
    srcs = ["ga.go"],
    importpath = "github.com/go-playground/locales/ga",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
