load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "haw",
    srcs = ["haw.go"],
    importpath = "github.com/go-playground/locales/haw",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":haw",
    visibility = ["//visibility:public"],
)

go_test(
    name = "haw_test",
    srcs = ["haw_test.go"],
    embed = [":haw"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
