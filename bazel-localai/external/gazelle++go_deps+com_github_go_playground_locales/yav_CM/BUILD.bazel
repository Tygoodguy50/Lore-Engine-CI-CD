load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "yav_CM",
    srcs = ["yav_CM.go"],
    importpath = "github.com/go-playground/locales/yav_CM",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":yav_CM",
    visibility = ["//visibility:public"],
)

go_test(
    name = "yav_CM_test",
    srcs = ["yav_CM_test.go"],
    embed = [":yav_CM"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
