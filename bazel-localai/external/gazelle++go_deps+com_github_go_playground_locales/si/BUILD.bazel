load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "si",
    srcs = ["si.go"],
    importpath = "github.com/go-playground/locales/si",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":si",
    visibility = ["//visibility:public"],
)

go_test(
    name = "si_test",
    srcs = ["si_test.go"],
    embed = [":si"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
