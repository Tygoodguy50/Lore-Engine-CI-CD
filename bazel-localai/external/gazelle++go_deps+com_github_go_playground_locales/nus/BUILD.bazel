load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "nus",
    srcs = ["nus.go"],
    importpath = "github.com/go-playground/locales/nus",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":nus",
    visibility = ["//visibility:public"],
)

go_test(
    name = "nus_test",
    srcs = ["nus_test.go"],
    embed = [":nus"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
