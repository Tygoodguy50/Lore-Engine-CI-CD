load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "fi",
    srcs = ["fi.go"],
    importpath = "github.com/go-playground/locales/fi",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":fi",
    visibility = ["//visibility:public"],
)

go_test(
    name = "fi_test",
    srcs = ["fi_test.go"],
    embed = [":fi"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
