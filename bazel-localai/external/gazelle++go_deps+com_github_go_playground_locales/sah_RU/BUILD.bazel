load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "sah_RU",
    srcs = ["sah_RU.go"],
    importpath = "github.com/go-playground/locales/sah_RU",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":sah_RU",
    visibility = ["//visibility:public"],
)

go_test(
    name = "sah_RU_test",
    srcs = ["sah_RU_test.go"],
    embed = [":sah_RU"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
