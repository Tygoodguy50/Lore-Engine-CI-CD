load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "am",
    srcs = ["am.go"],
    importpath = "github.com/go-playground/locales/am",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":am",
    visibility = ["//visibility:public"],
)

go_test(
    name = "am_test",
    srcs = ["am_test.go"],
    embed = [":am"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
