load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "ur",
    srcs = ["ur.go"],
    importpath = "github.com/go-playground/locales/ur",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
