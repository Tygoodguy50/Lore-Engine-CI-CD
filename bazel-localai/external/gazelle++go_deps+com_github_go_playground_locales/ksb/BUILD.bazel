load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "ksb",
    srcs = ["ksb.go"],
    importpath = "github.com/go-playground/locales/ksb",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
