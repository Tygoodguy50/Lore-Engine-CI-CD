load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "de",
    srcs = ["de.go"],
    importpath = "github.com/go-playground/locales/de",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":de",
    visibility = ["//visibility:public"],
)

go_test(
    name = "de_test",
    srcs = ["de_test.go"],
    embed = [":de"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
