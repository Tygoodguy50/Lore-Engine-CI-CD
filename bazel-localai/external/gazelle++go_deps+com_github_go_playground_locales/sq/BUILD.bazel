load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "sq",
    srcs = ["sq.go"],
    importpath = "github.com/go-playground/locales/sq",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
