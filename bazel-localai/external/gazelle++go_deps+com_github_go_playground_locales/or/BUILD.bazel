load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "or",
    srcs = ["or.go"],
    importpath = "github.com/go-playground/locales/or",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
