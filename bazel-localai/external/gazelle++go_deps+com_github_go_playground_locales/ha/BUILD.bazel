load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "ha",
    srcs = ["ha.go"],
    importpath = "github.com/go-playground/locales/ha",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":ha",
    visibility = ["//visibility:public"],
)

go_test(
    name = "ha_test",
    srcs = ["ha_test.go"],
    embed = [":ha"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
