load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "ff",
    srcs = ["ff.go"],
    importpath = "github.com/go-playground/locales/ff",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":ff",
    visibility = ["//visibility:public"],
)

go_test(
    name = "ff_test",
    srcs = ["ff_test.go"],
    embed = [":ff"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
