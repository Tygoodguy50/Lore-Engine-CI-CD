load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "th",
    srcs = ["th.go"],
    importpath = "github.com/go-playground/locales/th",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":th",
    visibility = ["//visibility:public"],
)

go_test(
    name = "th_test",
    srcs = ["th_test.go"],
    embed = [":th"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
