load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "ja",
    srcs = ["ja.go"],
    importpath = "github.com/go-playground/locales/ja",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":ja",
    visibility = ["//visibility:public"],
)

go_test(
    name = "ja_test",
    srcs = ["ja_test.go"],
    embed = [":ja"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
