load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "nb",
    srcs = ["nb.go"],
    importpath = "github.com/go-playground/locales/nb",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":nb",
    visibility = ["//visibility:public"],
)

go_test(
    name = "nb_test",
    srcs = ["nb_test.go"],
    embed = [":nb"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
