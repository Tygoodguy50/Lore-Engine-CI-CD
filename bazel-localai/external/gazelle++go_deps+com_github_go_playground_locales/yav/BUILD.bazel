load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "yav",
    srcs = ["yav.go"],
    importpath = "github.com/go-playground/locales/yav",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":yav",
    visibility = ["//visibility:public"],
)

go_test(
    name = "yav_test",
    srcs = ["yav_test.go"],
    embed = [":yav"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
