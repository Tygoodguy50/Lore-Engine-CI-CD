load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "zgh",
    srcs = ["zgh.go"],
    importpath = "github.com/go-playground/locales/zgh",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":zgh",
    visibility = ["//visibility:public"],
)

go_test(
    name = "zgh_test",
    srcs = ["zgh_test.go"],
    embed = [":zgh"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
