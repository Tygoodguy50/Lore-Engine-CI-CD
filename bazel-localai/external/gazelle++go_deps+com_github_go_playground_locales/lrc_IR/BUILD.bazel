load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "lrc_IR",
    srcs = ["lrc_IR.go"],
    importpath = "github.com/go-playground/locales/lrc_IR",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":lrc_IR",
    visibility = ["//visibility:public"],
)

go_test(
    name = "lrc_IR_test",
    srcs = ["lrc_IR_test.go"],
    embed = [":lrc_IR"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
