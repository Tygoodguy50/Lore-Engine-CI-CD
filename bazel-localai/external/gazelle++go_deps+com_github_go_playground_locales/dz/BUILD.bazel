load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "dz",
    srcs = ["dz.go"],
    importpath = "github.com/go-playground/locales/dz",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":dz",
    visibility = ["//visibility:public"],
)

go_test(
    name = "dz_test",
    srcs = ["dz_test.go"],
    embed = [":dz"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
