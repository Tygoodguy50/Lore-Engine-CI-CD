load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "vo",
    srcs = ["vo.go"],
    importpath = "github.com/go-playground/locales/vo",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
