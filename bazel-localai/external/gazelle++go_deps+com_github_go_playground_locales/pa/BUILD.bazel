load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "pa",
    srcs = ["pa.go"],
    importpath = "github.com/go-playground/locales/pa",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":pa",
    visibility = ["//visibility:public"],
)

go_test(
    name = "pa_test",
    srcs = ["pa_test.go"],
    embed = [":pa"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
