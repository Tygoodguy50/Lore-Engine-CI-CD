load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "my",
    srcs = ["my.go"],
    importpath = "github.com/go-playground/locales/my",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":my",
    visibility = ["//visibility:public"],
)

go_test(
    name = "my_test",
    srcs = ["my_test.go"],
    embed = [":my"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
