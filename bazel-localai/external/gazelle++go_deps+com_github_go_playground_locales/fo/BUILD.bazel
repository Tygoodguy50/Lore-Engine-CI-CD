load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "fo",
    srcs = ["fo.go"],
    importpath = "github.com/go-playground/locales/fo",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":fo",
    visibility = ["//visibility:public"],
)

go_test(
    name = "fo_test",
    srcs = ["fo_test.go"],
    embed = [":fo"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
