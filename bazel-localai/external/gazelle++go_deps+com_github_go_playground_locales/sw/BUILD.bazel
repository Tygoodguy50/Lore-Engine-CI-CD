load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "sw",
    srcs = ["sw.go"],
    importpath = "github.com/go-playground/locales/sw",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":sw",
    visibility = ["//visibility:public"],
)

go_test(
    name = "sw_test",
    srcs = ["sw_test.go"],
    embed = [":sw"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
