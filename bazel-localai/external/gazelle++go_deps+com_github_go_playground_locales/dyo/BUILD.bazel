load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "dyo",
    srcs = ["dyo.go"],
    importpath = "github.com/go-playground/locales/dyo",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":dyo",
    visibility = ["//visibility:public"],
)

go_test(
    name = "dyo_test",
    srcs = ["dyo_test.go"],
    embed = [":dyo"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
