load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "el",
    srcs = ["el.go"],
    importpath = "github.com/go-playground/locales/el",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":el",
    visibility = ["//visibility:public"],
)

go_test(
    name = "el_test",
    srcs = ["el_test.go"],
    embed = [":el"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
