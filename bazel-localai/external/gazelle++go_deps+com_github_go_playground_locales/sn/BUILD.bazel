load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "sn",
    srcs = ["sn.go"],
    importpath = "github.com/go-playground/locales/sn",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
