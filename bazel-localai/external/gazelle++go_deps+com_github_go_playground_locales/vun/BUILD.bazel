load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "vun",
    srcs = ["vun.go"],
    importpath = "github.com/go-playground/locales/vun",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":vun",
    visibility = ["//visibility:public"],
)

go_test(
    name = "vun_test",
    srcs = ["vun_test.go"],
    embed = [":vun"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
