load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "seh",
    srcs = ["seh.go"],
    importpath = "github.com/go-playground/locales/seh",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":seh",
    visibility = ["//visibility:public"],
)

go_test(
    name = "seh_test",
    srcs = ["seh_test.go"],
    embed = [":seh"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
