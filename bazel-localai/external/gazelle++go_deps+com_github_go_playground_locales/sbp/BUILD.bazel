load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "sbp",
    srcs = ["sbp.go"],
    importpath = "github.com/go-playground/locales/sbp",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":sbp",
    visibility = ["//visibility:public"],
)

go_test(
    name = "sbp_test",
    srcs = ["sbp_test.go"],
    embed = [":sbp"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
