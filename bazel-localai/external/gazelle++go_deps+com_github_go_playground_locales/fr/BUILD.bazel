load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "fr",
    srcs = ["fr.go"],
    importpath = "github.com/go-playground/locales/fr",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":fr",
    visibility = ["//visibility:public"],
)

go_test(
    name = "fr_test",
    srcs = ["fr_test.go"],
    embed = [":fr"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
