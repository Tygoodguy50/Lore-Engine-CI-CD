load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "tt",
    srcs = ["tt.go"],
    importpath = "github.com/go-playground/locales/tt",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":tt",
    visibility = ["//visibility:public"],
)

go_test(
    name = "tt_test",
    srcs = ["tt_test.go"],
    embed = [":tt"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
