load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "nnh",
    srcs = ["nnh.go"],
    importpath = "github.com/go-playground/locales/nnh",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":nnh",
    visibility = ["//visibility:public"],
)

go_test(
    name = "nnh_test",
    srcs = ["nnh_test.go"],
    embed = [":nnh"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
