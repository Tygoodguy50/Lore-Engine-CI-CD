load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "mfe",
    srcs = ["mfe.go"],
    importpath = "github.com/go-playground/locales/mfe",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":mfe",
    visibility = ["//visibility:public"],
)

go_test(
    name = "mfe_test",
    srcs = ["mfe_test.go"],
    embed = [":mfe"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
