load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "nmg",
    srcs = ["nmg.go"],
    importpath = "github.com/go-playground/locales/nmg",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":nmg",
    visibility = ["//visibility:public"],
)

go_test(
    name = "nmg_test",
    srcs = ["nmg_test.go"],
    embed = [":nmg"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
