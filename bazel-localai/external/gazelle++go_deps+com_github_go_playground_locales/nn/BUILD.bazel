load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "nn",
    srcs = ["nn.go"],
    importpath = "github.com/go-playground/locales/nn",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":nn",
    visibility = ["//visibility:public"],
)

go_test(
    name = "nn_test",
    srcs = ["nn_test.go"],
    embed = [":nn"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
