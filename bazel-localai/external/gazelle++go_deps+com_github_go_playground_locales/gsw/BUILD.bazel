load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "gsw",
    srcs = ["gsw.go"],
    importpath = "github.com/go-playground/locales/gsw",
    visibility = ["//visibility:public"],
    deps = [
        "//:locales",
        "//currency",
    ],
)

alias(
    name = "go_default_library",
    actual = ":gsw",
    visibility = ["//visibility:public"],
)

go_test(
    name = "gsw_test",
    srcs = ["gsw_test.go"],
    embed = [":gsw"],
    deps = [
        "//:locales",
        "//currency",
    ],
)
