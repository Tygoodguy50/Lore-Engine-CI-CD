load("@rules_proto//proto:defs.bzl", "proto_library")
load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

proto_library(
    name = "protoexample_proto",
    srcs = ["test.proto"],
    visibility = ["//visibility:public"],
)

go_proto_library(
    name = "protoexample_go_proto",
    importpath = "github.com/gin-gonic/gin/testdata/protoexample",
    proto = ":protoexample_proto",
    visibility = ["//visibility:public"],
)

go_library(
    name = "protoexample",
    embed = [":protoexample_go_proto"],
    importpath = "github.com/gin-gonic/gin/testdata/protoexample",
    visibility = ["//visibility:public"],
)

alias(
    name = "go_default_library",
    actual = ":protoexample",
    visibility = ["//visibility:public"],
)
