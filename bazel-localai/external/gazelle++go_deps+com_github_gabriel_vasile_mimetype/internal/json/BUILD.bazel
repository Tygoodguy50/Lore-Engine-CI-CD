load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "json",
    srcs = ["json.go"],
    importpath = "github.com/gabriel-vasile/mimetype/internal/json",
    visibility = ["//:__subpackages__"],
)

alias(
    name = "go_default_library",
    actual = ":json",
    visibility = ["//:__subpackages__"],
)

go_test(
    name = "json_test",
    srcs = ["json_test.go"],
    embed = [":json"],
)
