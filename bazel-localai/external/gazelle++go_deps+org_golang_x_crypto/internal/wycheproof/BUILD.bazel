load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "wycheproof",
    srcs = ["notboring.go"],
    importpath = "golang.org/x/crypto/internal/wycheproof",
    visibility = ["//:__subpackages__"],
)

alias(
    name = "go_default_library",
    actual = ":wycheproof",
    visibility = ["//:__subpackages__"],
)

go_test(
    name = "wycheproof_test",
    srcs = [
        "aead_test.go",
        "aes_cbc_test.go",
        "dsa_test.go",
        "ecdh_stdlib_test.go",
        "ecdh_test.go",
        "ecdsa_test.go",
        "eddsa_test.go",
        "hkdf_test.go",
        "hmac_test.go",
        "rsa_oaep_decrypt_test.go",
        "rsa_pss_test.go",
        "rsa_signature_test.go",
        "wycheproof_test.go",
    ],
    embed = [":wycheproof"],
    deps = [
        "//chacha20poly1305",
        "//cryptobyte",
        "//cryptobyte/asn1",
        "//ed25519",
        "//hkdf",
        "//internal/wycheproof/internal/dsa",
    ],
)
