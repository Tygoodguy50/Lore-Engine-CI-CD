load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "httpguts",
    srcs = [
        "guts.go",
        "httplex.go",
    ],
    importpath = "golang.org/x/net/http/httpguts",
    visibility = ["//visibility:public"],
    deps = ["//idna"],
)

alias(
    name = "go_default_library",
    actual = ":httpguts",
    visibility = ["//visibility:public"],
)

go_test(
    name = "httpguts_test",
    srcs = ["httplex_test.go"],
    embed = [":httpguts"],
)
