load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "socks",
    srcs = [
        "client.go",
        "socks.go",
    ],
    importpath = "golang.org/x/net/internal/socks",
    visibility = ["//:__subpackages__"],
)

alias(
    name = "go_default_library",
    actual = ":socks",
    visibility = ["//:__subpackages__"],
)

go_test(
    name = "socks_test",
    srcs = ["dial_test.go"],
    deps = [
        ":socks",
        "//internal/sockstest",
    ],
)
